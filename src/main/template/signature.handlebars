<h4 class="sample-title" data-control data-toggle="collapse"
    data-target="#sample-{{id}}">{{type}} Sample</h4>
<div data-content class="collapse in" id="sample-{{id}}">
    <div class="snippet">
        <pre><code>{{sampleJSON}}</code></pre>
    </div>
</div>

{{#if signature}}
    <h4 class="schema-title collapsed" data-control data-toggle="collapse"
        data-target="#schema-{{id}}">{{type}} Schema</h4>
    <div data-content class="collapse" id="schema-{{id}}">
        <div class="description">
            {{{signature}}}
        </div>
    </div>
{{/if}}


<h4 class="code-sample-title" data-control data-toggle="collapse" data-target="#code-sample-{{id}}">{{type}} Code Sample</h4>
<div data-content class="collapse in" id="code-sample-{{id}}">
    <div class="tabs-{{id}}">
        <ul>
            <li>Python</li>
            <li>PHP</li>
            <li>Ruby</li>
            <li>.NET</li>
            <li>JAVA</li>
        </ul>
        <div>
            <div class="python">
                <pre><code>python</code></pre>
            </div>
            <div class="php">
                <pre><code>php</code></pre>
            </div>
            <div class="ruby">
                <pre><code>ruby</code></pre>
            </div>
            <div class="net">
                <pre><code>net</code></pre>
            </div>
            <div class="java">
                <pre><code>java</code></pre>
            </div>
        </div>
    </div>
</div>
